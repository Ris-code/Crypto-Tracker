{
    "version": 2,
    "name": "crypto-tracker",
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/swagger.json",
            "dest": "src/app.js"
        },
        {
            "src": "/api-docs/(.*)",
            "dest": "src/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/app.js"
        }
    ]
}